<% include header %>
<script src="/components/jquery/dist/jquery.min.js" type="text/javascript"></script>
  <h1><%-title %></h1>
  <p>Welcome to <%-title %></p>
  <p id="ip"></p>
  <p id="hostname"></p>
  <p id="city"></p>
  <p id="region"></p>
  <p id="country"></p>
  <p id="loc"></p>
  <p id="org"></p>
  <script>
	$.get("http://ipinfo.io", function(response) {
	$("#ip").html(response.ip)
	$("#hostname").html(response.hostname)
	$("#city").html(response.city)
	$("#region").html(response.region)
	$("#country").html(response.country)
	$("#loc").html(response.loc)
	$("#org").html(response.org)
	}, "jsonp")
  </script>
<% include footer %>